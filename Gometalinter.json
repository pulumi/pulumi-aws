{
    "Deadline": "5m",
    "DisableAll": true,
    "Enable": [
        "deadcode",
        "errcheck",
        "gas",
        "goconst",
        "gofmt",
        "golint",
        "gosimple",
        "ineffassign",
        "interfacer",
        "lll",
        "misspell",
        "staticcheck",
        "structcheck",
        "unconvert",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "Exclude": [
        "/usr/local/go"
    ],
    "LineLength": 120
}

